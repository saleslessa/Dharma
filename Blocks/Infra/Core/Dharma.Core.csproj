    </Compile>
  </ItemGroup>
  <ItemGroup>
    <None Remove="Gateway\Gateway.config" />
  </ItemGroup>
  <ProjectExtensions>
    <MonoDevelop>
      <Properties>
        <Policies>
          <TextStylePolicy inheritsSet="null" scope="text/x-csharp" />
